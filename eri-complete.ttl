# Scroll Phases
igbo:planting
  rdf:type eri:ScrollPhase ;
  rdfs:label "Planting (Ịkụ mkpụrụ)" ;
  eri:coherence "< 0.4" ;
  rdfs:comment "Beginning state, low coherence, scattered energy" .

igbo:watering
  rdf:type eri:ScrollPhase ;
  rdfs:label "Watering (Ịgba mmiri)" ;
  eri:coherence "> 0.4" ;
  rdfs:comment "Active growth, yang-dominant energy" .

igbo:pruning
  rdf:type eri:ScrollPhase ;
  rdfs:label "Pruning (Ịkwacha)" ;
  eri:coherence "> 0.6" ;
  rdfs:comment "Refinement, yin-dominant energy, letting go" .

igbo:harvest
  rdf:type eri:ScrollPhase ;
  rdfs:label "Harvest (Ịchịkọta)" ;
  eri:coherence "> 0.7" ;
  rdfs:comment "Integration, balanced energy, wholeness" .

