@prefix ex: <http://elresonance.ai/data#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Sample Instance Data - Demonstrating ERI in Action

# Example 1: Igbo Utterance - Low Coherence (Planting Phase)
ex:utterance_001
  a eri:Utterance ;
  eri:text "Obi m adịghị mma" ;
  eri:detectedLanguage "igbo" ;
  eri:hasResonanceField ex:field_001 .

ex:field_001
  a eri:ResonanceField ;
  eri:coherence "0.32"^^xsd:decimal ;
  eri:coherenceTrend "fragmenting" ;
  eri:scrollPhase igbo:planting ;
  eri:spectrum eri:reflecting ;
  eri:dominantVowel igbo:vowel_ị ;
  eri:spectrumDescription "Turning inward, heart-mind misalignment" .

# Example 2: Yoruba Utterance - High Coherence (Harvest Phase)
ex:utterance_002
  a eri:Utterance ;
  eri:text "Ó dára púpọ̀" ;
  eri:detectedLanguage "yoruba" ;
  eri:hasResonanceField ex:field_002 .

ex:field_002
  a eri:ResonanceField ;
  eri:coherence "0.82"^^xsd:decimal ;
  eri:coherenceTrend "harmonizing" ;
  eri:scrollPhase igbo:harvest ;
  eri:spectrum eri:expanding ;
  eri:dominantVowel yoruba:vowel_a ;
  eri:spectrumDescription "Opening to possibilities, balanced energy" .

# Example 3: English Utterance - Moderate Coherence (Watering Phase)
ex:utterance_003
  a eri:Utterance ;
  eri:text "I feel like I am being watched on my digital field" ;
  eri:detectedLanguage "english" ;
  eri:hasResonanceField ex:field_003 .

ex:field_003
  a eri:ResonanceField ;
  eri:coherence "0.58"^^xsd:decimal ;
  eri:coherenceTrend "emerging" ;
  eri:scrollPhase igbo:watering ;
  eri:spectrum eri:clarifying ;
  eri:dominantVowel igbo:vowel_i ;
  eri:spectrumDescription "Sharp focus emerging from confusion" .

# Example 4: Igbo Utterance - Refinement Phase
ex:utterance_004
  a eri:Utterance ;
  eri:text "Ọ bụ ihe dị mma" ;
  eri:detectedLanguage "igbo" ;
  eri:hasResonanceField ex:field_004 .

ex:field_004
  a eri:ResonanceField ;
  eri:coherence "0.74"^^xsd:decimal ;
  eri:coherenceTrend "stable" ;
  eri:scrollPhase igbo:pruning ;
  eri:spectrum eri:grounding ;
  eri:dominantVowel igbo:vowel_ọ ;
  eri:spectrumDescription "Connecting with foundation, rooted presence" .

# Example 5: Session Tracking - Harmonizing Trend
ex:session_001
  a eri:ResonanceSession ;
  eri:hasUtterance ex:utterance_001, ex:utterance_003, ex:utterance_004 ;
  eri:sessionTrend "harmonizing" ;
  eri:coherenceProgression "0.32 → 0.58 → 0.74" ;
  eri:phaseTransition "planting → watering → pruning" .

