@prefix eri: <http://elresonance.ai/ontology#> .
@prefix igbo: <http://elresonance.ai/igbo#> .
@prefix yoruba: <http://elresonance.ai/yoruba#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology Declaration
<http://elresonance.ai/ontology#>
  rdf:type owl:Ontology ;
  rdfs:label "El Resonance Intelligence Ontology" ;
  rdfs:comment "Cultural-emotional middleware transforming indigenous vowel systems into queryable knowledge graphs" ;
  owl:versionInfo "1.0" .

# Classes
eri:ResonanceField
  rdf:type owl:Class ;
  rdfs:label "Resonance Field" ;
  rdfs:comment "Multi-dimensional emotional-linguistic state derived from vowel analysis" .

eri:Vowel
  rdf:type owl:Class ;
  rdfs:label "Vowel" ;
  rdfs:comment "Phonosemantic unit with energy, direction, and polarity properties" .

eri:ScrollPhase
  rdf:type owl:Class ;
  rdfs:label "Scroll Phase" ;
  rdfs:comment "Cultural reasoning primitive representing state transitions" .

eri:Spectrum
  rdf:type owl:Class ;
  rdfs:label "Resonance Spectrum" ;
  rdfs:comment "Fluid emotional position determined by vowel patterns" .

eri:Utterance
  rdf:type owl:Class ;
  rdfs:label "Utterance" ;
  rdfs:comment "Natural language input with resonance analysis" .

# Properties
eri:coherence
  rdf:type owl:DatatypeProperty ;
  rdfs:domain eri:ResonanceField ;
  rdfs:range xsd:decimal ;
  rdfs:label "Coherence" ;
  rdfs:comment "Heart-mind alignment measure (0.0-1.0) via Shannon entropy" .

eri:coherenceTrend
  rdf:type owl:DatatypeProperty ;
  rdfs:domain eri:ResonanceField ;
  rdfs:range xsd:string ;
  rdfs:label "Coherence Trend" ;
  rdfs:comment "harmonizing, fragmenting, stable, or emerging" .

eri:scrollPhase
  rdf:type owl:ObjectProperty ;
  rdfs:domain eri:ResonanceField ;
  rdfs:range eri:ScrollPhase ;
  rdfs:label "Scroll Phase" .

eri:spectrum
  rdf:type owl:ObjectProperty ;
  rdfs:domain eri:ResonanceField ;
  rdfs:range eri:Spectrum ;
  rdfs:label "Resonance Spectrum" .

eri:dominantVowel
  rdf:type owl:ObjectProperty ;
  rdfs:domain eri:ResonanceField ;
  rdfs:range eri:Vowel ;
  rdfs:label "Dominant Vowel" .

eri:energy
  rdf:type owl:DatatypeProperty ;
  rdfs:domain eri:Vowel ;
  rdfs:range xsd:string ;
  rdfs:label "Energy Type" ;
  rdfs:comment "expansive, sharp, deep, forward, inward, balanced, grounded, flowing" .

eri:direction
  rdf:type owl:DatatypeProperty ;
  rdfs:domain eri:Vowel ;
  rdfs:range xsd:string ;
  rdfs:label "Direction" ;
  rdfs:comment "outward, focused, downward, ascending, descending, centered, rooted, continuous" .

eri:polarity
  rdf:type owl:DatatypeProperty ;
  rdfs:domain eri:Vowel ;
  rdfs:range xsd:string ;
  rdfs:label "Polarity" ;
  rdfs:comment "yang, yin, or neutral" .

eri:text
  rdf:type owl:DatatypeProperty ;
  rdfs:domain eri:Utterance ;
  rdfs:range xsd:string ;
  rdfs:label "Text" .

eri:detectedLanguage
  rdf:type owl:DatatypeProperty ;
  rdfs:domain eri:Utterance ;
  rdfs:range xsd:string ;
  rdfs:label "Detected Language" .

eri:hasResonanceField
  rdf:type owl:ObjectProperty ;
  rdfs:domain eri:Utterance ;
  rdfs:range eri:ResonanceField ;
  rdfs:label "Has Resonance Field" .

